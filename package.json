{
  "name": "porn-manager",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "watch": "tsc -w",
    "dev": "DEBUG=log,success,warn,error,http node build/",
    "test": "exit 1",
    "build": "pkg . --out-path ./release/build",
    "build:app": "exit 1",
    "build:all": "npm run build:linux64 && npm run build:win_all && npm run build:mac",
    "build:linux64": "pkg . --out-path ./release/linux64 --targets node10-linux-x64",
    "build:win_all": "npm run build:win64 && npm run build:win86",
    "build:win64": "pkg . --out-path ./release/win64 --targets node10-win-x64",
    "build:win86": "pkg . --out-path ./release/win86 --targets node10-win-x86",
    "build:mac": "pkg . --out-path ./release/macos --targets node10-macos-x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boi123212321/porn-manager.git"
  },
  "author": "boi123212321",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boi123212321/porn-manager/issues"
  },
  "homepage": "https://github.com/boi123212321/porn-manager#readme",
  "dependencies": {
    "@types/cli-progress": "^1.8.1",
    "@types/graphql": "^14.5.0",
    "@types/node": "^12.12.7",
    "apollo-server-express": "^2.9.8",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cli-progress": "^3.3.1",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "graphql-tools": "^4.0.6",
    "graphql-type-long": "^0.1.1",
    "inquirer": "6.3.1",
    "js-sha512": "^0.8.0",
    "lowdb": "^1.0.0",
    "mkdirp": "^0.5.1",
    "tiny-async-pool": "^1.0.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/fluent-ffmpeg": "^2.1.11",
    "@types/inquirer": "^6.5.0",
    "@types/lowdb": "^1.0.9",
    "@types/mkdirp": "^0.5.2",
    "nodemon": "^1.19.4",
    "pkg": "^4.4.0"
  },
  "nodemonConfig": {
    "ignore": [
      "/**/db.json",
      "/**/config.json"
    ]
  }
}
